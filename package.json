{
  "name": "@kalisio/kdk",
  "description": "Kalisio Development Kit",
  "version": "1.1.0",
  "homepage": "https://github.com/kalisio/kdk",
  "main": "lib/",
  "keywords": [
    "Kalisio",
    "KDK"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/kdk.git"
  },
  "author": {
    "name": "Kalisio contributors",
    "url": "https://github.com/kalisio/kdk"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/kalisio/kdk/issues"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "release:patch": "npm version patch && npm publish --access=public",
    "release:minor": "npm version minor && npm publish --access=public",
    "release:major": "npm version major && npm publish --access=public",
    "changelog": "github_changelog_generator -u kalisio -p kdk && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "compile:core": "shx rm -rf lib/core && babel -d lib/core core/ -s --copy-files",
    "compile:map": "shx rm -rf lib/map && babel -d lib/map map/ -s --copy-files",
    "compile": "npm-run-all --serial compile:**",
    "watch:core:js": "babel --watch -d lib/core core/ --copy-files",
    "watch:core:vue": "cpx core/**/*.vue lib/core --watch",
    "watch:map:js": "babel --watch -d lib/map map/ --copy-files",
    "watch:map:vue": "cpx map/**/*.vue lib/map --watch",
    "watch": "npm-run-all --parallel watch:**",
    "lint:core": "standard core/**/*.js core/**/*.vue tests/core/**/*.js --fix",
    "lint:map": "standard map/**/*.js map/**/*.vue tests/map/**/*.js --fix",
    "lint": "npm-run-all --serial lint:**",
    "mocha:core": "cross-env NODE_CONFIG_DIR=./tests/core/config/ mocha tests/core/**/*.test.js --require babel-core/register",
    "mocha:map": "cross-env NODE_CONFIG_DIR=./tests/map/config/ mocha tests/map/**/*.test.js --require babel-core/register",
    "mocha": "npm-run-all --serial mocha:**",
    "coverage": "nyc npm run mocha",
    "test": "npm run compile && npm run lint && npm run coverage"
  },
  "standard": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "plugins": [
      "vue"
    ],
    "env": [
      "mocha"
    ],
    "globals": [
      "DEV",
      "PROD",
      "fetch",
      "cordova",
      "location",
      "FileReader",
      "LocalFileSystem",
      "mime",
      "atob",
      "URL",
      "Blob",
      "Event"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@feathersjs/authentication": "https://github.com/kalisio/authentication.git",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/authentication-local": "^1.2.1",
    "@feathersjs/authentication-oauth2": "^1.1.0",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/socketio": "^3.2.2",
    "abort-controller": "^3.0.0",
    "aws-sdk": "^2.200.0",
    "body-parser": "^1.17.1",
    "casl": "^1.0.3",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "cron": "^1.4.1",
    "dauria": "^2.0.0",
    "debug": "^4.1.0",
    "ejs": "^2.5.7",
    "email-templates": "^2.7.1",
    "express-rate-limit": "^2.11.0",
    "feathers-authentication-management": "^2.0.1",
    "feathers-blob": "^1.4.0",
    "feathers-hooks-common": "^3.0.0",
    "feathers-mailer": "^2.0.0",
    "feathers-mongodb": "^2.8.0",
    "feathers-mongodb-management": "^0.1.0",
    "geotiff": "^1.0.0-beta.6",
    "helmet": "^3.5.0",
    "jsdap": "^8.1.0",
    "limiter": "^1.1.3",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "moment": "^2.24.0",
    "mongodb": "^2.2.26",
    "multer": "^1.3.0",
    "node-fetch": "^2.6.0",
    "node-geocoder": "^3.21.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-oauth2-cognito": "^0.1.0",
    "password-generator": "^2.2.0",
    "password-validator": "^4.0.0",
    "s3-blob-store": "2.1.0",
    "sift": "^8.0.0",
    "sns-mobile": "https://github.com/kalisio/sns-mobile#protocol",
    "superagent": "^5.1.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@kalisio/feathers-distributed": "^0.7.1",
    "@turf/intersect": "^6.1.3",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.17.1",
    "chai": "^4.2.0",
    "chai-lint": "^0.1.1",
    "chai-spies": "^0.7.1",
    "containerized": "^1.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "feathers-memory": "^2.2.0",
    "fs-extra": "^8.1.0",
    "googleapis": "^21.3.0",
    "mocha": "^6.2.0",
    "nock": "^9.2.6",
    "npm-run-all": "^4.1.1",
    "nyc": "^15.0.1",
    "shx": "^0.3.2",
    "standard": "^14.0.0",
    "superagent": "^5.1.0",
    "weacast-core": "^1.1.0",
    "weacast-gfs": "^1.1.0",
    "weacast-probe": "^1.1.0"
  },
  "peerDependencies": {}
}
