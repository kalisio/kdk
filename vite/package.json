  "type": "module",
  "files": [
    "client"
  ],
  "main": "./client/kdk.client.umd.cjs",
  "module": "./client/kdk.client.js",
  "exports": {
    ".": {
      "import": "./client/kdk.client.js",
      "require": "./client/kdk.client.umd.cjs"
    },
    "./style.css": "./client/kdk.client.css"
  },
  "keywords": [
    "Kalisio",
    "KDK"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/kalisio/kdk.git"
